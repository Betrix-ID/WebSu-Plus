                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="Output TYPE_COMMAND"
                    android:textSize="15sp"
                    android:textColor="@color/colorOnPrimary"/>

                <CheckBox
