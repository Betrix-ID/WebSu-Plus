                    android:checked="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"/>

            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/bg_rounded_top"
        android:elevation="4dp"
        android:padding="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp"
            android:gravity="center_vertical"
            android:id="@+id/filter_delete_header"
            android:clickable="true"
            android:foreground="?attr/selectableItemBackground">

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/ic_filter_delete"
                android:tint="#FF5484A6"
                android:contentDescription="delete_icon"/>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_marginStart="16dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Filter delete "
                    android:textSize="17sp"
                    android:textColor="@color/colorOnPrimary"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="show filter to delete"
                    android:textSize="13sp"
                    android:textColor="@color/text_primary"/>

            </LinearLayout>

            <ImageView
                android:id="@+id/icon_dropdown_delete"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:src="@drawable/ic_up"
                android:tint="#FF5484A6"
                android:contentDescription="dropdown icon"/>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/filter_delete_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="visible">

            <RelativeLayout
                android:id="@+id/rl_delete_command"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                android:paddingStart="16dp"
                android:paddingEnd="16dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="22dp"
                android:layout_marginEnd="22dp"
                android:layout_marginBottom="6dp">

                <TextView
                    android:id="@+id/tv_delete_command"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="Deleting TEXT_COMMAND"
                    android:textSize="15sp"
                    android:textColor="@color/colorOnPrimary"/>

                <CheckBox
                    android:id="@+id/cb_delete_command"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:checked="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_delete_output"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                android:paddingStart="16dp"
                android:paddingEnd="16dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="22dp"
                android:layout_marginEnd="22dp"
                android:layout_marginBottom="6dp">

                <TextView
                    android:id="@+id/tv_delete_output"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="Deleting TEXT_OUTPUT"
                    android:textSize="15sp"
                    android:textColor="@color/colorOnPrimary"/>

                <CheckBox
                    android:id="@+id/cb_delete_output"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:checked="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rl_delete_input"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                android:paddingStart="16dp"
                android:paddingEnd="16dp"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:layout_marginStart="22dp"
                android:layout_marginEnd="22dp"
                android:layout_marginBottom="14dp">

                <TextView
                    android:id="@+id/tv_delete_input"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="Deleting TEXT_INPUT"
                    android:textSize="15sp"
                    android:textColor="@color/colorOnPrimary"/>

                <CheckBox
                    android:id="@+id/cb_delete_input"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:checked="true"
                    android:minWidth="0dp"
                    android:minHeight="0dp"/>

            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

